{
    "gitdoc.enabled": true,
    "autoHeader": {
    "format": {
        "startWith": "/**",
        "middleWith": "*",
        "endWith": "*/",
        "headerPrefix": "@",
    },
    "header": {
        "File Name": "",
        "Author": "Alexander Karl",
        "Create Time": {
            "type": "createTime",
            "format": "YYYY-MM-DD",
        },
        "Description": "",
    }
    }  
}